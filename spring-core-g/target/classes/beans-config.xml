<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<bean class="com.te.spring.core.dto.Address" name="address">
		<property name="addressLine" value="Address 01"/>
	</bean>
	
	<bean class="com.te.spring.core.dto.Student" name="student01" autowire="byName" >
		<property name="studentId" value="1" />
		<property name="studentName" value="Student 01" />
	</bean>
</beans>